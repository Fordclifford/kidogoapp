{
  "expo": {
    "name": "KidogoApp",
    "slug": "KidogoApp",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kidogoApp.kidogoApp",
      "icon": "./assets/images/ios.png"
    },
    "android": {
      "package": "com.kidogoApp.kidogoApp",
      "versionCode": 2
    }
  }
}
